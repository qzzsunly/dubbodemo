<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:c="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <dubbo:service id="demoService1" interface="life.qzz.dubbodemo.api.DemoService" ref="demoService" dynamic="false" version="1.1.1"/>
    <dubbo:service id="demoService2" interface="life.qzz.dubbodemo.api.DemoService" ref="demoService" version="1.1.2"/>

    <bean id="demoService" class="life.qzz.dubbodemo.api.impl.DemoServiceImpl"/>

    <c:annotation-config/>
